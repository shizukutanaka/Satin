{
    "enabled": true,
    "settings": {
        "log_level": "info",  // デフォルトのログレベル
        "max_bytes": 10485760,  // ログファイルの最大サイズ（10MB）
        "backup_count": 10,  // 保持するバックアップファイル数
        "compression_interval": 1800,  // 圧縮間隔（秒）
        "log_format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
        "date_format": "%Y-%m-%d %H:%M:%S",
        "log_file": "satin.log",
        "log_dir": "logs"
    }
}
